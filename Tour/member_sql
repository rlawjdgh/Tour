create sequence seq_member;
	
create table member (
		idx number(5) primary key,
		name varchar2(20),
		id varchar2(10),
		email varchar2(50) not null unique,
		password varchar2(20),
		phone number(30), 
		grade varchar2(20),
		authkey varchar2(50),
		authstatus number(5),
		point number(30),
		coupon number(10), 
		admin number(2)
	); 